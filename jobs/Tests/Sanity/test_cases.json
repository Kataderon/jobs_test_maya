    {
        "case": "MAYA_SAN_007",
        "status": "active",
        "functions": [
            "rpr = cmd.shadingNode('lambert', asShader=True)",
            "cmd.setAttr(rpr + '.color', 1, 0, 1)",
            "sge = rpr + 'SG'",
            "cmd.sets(renderable=True, noSurfaceShader=True, empty=True, name=sge)",
            "cmd.connectAttr((rpr + '.outColor'), (sge + '.surfaceShader'), f=True)",
            "cmd.select('shaderball', r=True)",
            "sgs = mel.eval('listConnections -type shadingEngine \"' + rpr + '\"')",
            "cmd.sets(e=True, forceElement=sgs[0])",
            "cmd.setAttr('RadeonProRenderGlobals.completionCriteriaIterations',  10)",
            "cmd.optionVar(iva=('RPR_DevicesSelected', ( \"gpu\" in ['gpu', 'dual'])))",
            "cmd.optionVar(iva=('RPR_DevicesSelected', ( \"gpu\" in ['cpu', 'dual'])))",
            "rpr_render(test_case, script_info)"
        ],
        "script_info": [
            "Lambert material test",
