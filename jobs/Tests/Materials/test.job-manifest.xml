<?xml version="1.0" ?>
<job-manifest enable="False">

    <outpath value="{OutputDir}"/>

    <execute command='python "{CWD}\Scripts\simpleRender.py"' stage="stage1">
        <argument>--stage_report stage1.json</argument>
        <argument>--tool "{Tool}"</argument>
        <argument>--tests "{CWD}\Tests\Materials\{TestsFilter}TestsList.txt"</argument>
        <argument>--gpumode "{RenderDevice}"</argument>
        <argument>--output "{OutputDir}"</argument>
        <argument>--testType Materials</argument>
        <argument>--projectPath "{ScenesDir}"</argument>
    </execute>

    <execute command='python "{CommonDir}\scripts\checkExpected.py"' stage="stage2">
        <argument>--stage_report stage2.json</argument>
        <argument>--work_dir "{OutputDir}"</argument>
        <argument>--report_name expected.json</argument>
        <argument>--result_name not_rendered.json</argument>
    </execute>

    <execute command='python "{CommonDir}\scripts\ImageComparator\compareByJSON.py"' stage="stage3">
        <argument>--stage_report stage3.json</argument>
        <argument>--work_dir "{OutputDir}"</argument>
        <argument>--base_dir "{BaselineDir}"</argument>
        <argument>--report_name report_mel.json</argument>
        <argument>--result_name report_compare.json</argument>
    </execute>

    <execute command='python "{CommonDir}\scripts\HtmlTemplate\htmlBuilder.py"' stage="stage4">
        <argument>--stage_report stage4.json</argument>
        <argument>--work_dir "{OutputDir}"</argument>
        <argument>--template_name template_maya_without_opacity.html</argument>
    </execute>

</job-manifest>
