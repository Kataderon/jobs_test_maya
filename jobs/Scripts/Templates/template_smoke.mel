global proc create_ies_light(){{	
	source shelfCommands.mel; createIESLight();
	setAttr -type "string" "RPRIESLight.iesFile" ("{res_path}" + "/sourceimages/1.IES");
	select -r RPRIES1;
	move -r 0 400 0;
	rotate -r -os -fo 0 90 0;
	setAttr "RPRIESLight.intensity" 20;
}}

