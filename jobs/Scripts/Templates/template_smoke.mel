
global proc check_test_cases_fail_save(string $test_case, int $pass, string $script_info, string $scene){{

    string $tests = "{testCases}";
    string $test[];
    $numTokens = `tokenize $tests "," $test`;

    if ($tests != "all") {{
        for ($each in $test) {{ 
            if ($each == $test_case) {{
                rpr_fail_save($test_case, $script_info);
            }}
        }}
    }} else {{
        rpr_fail_save($test_case, $script_info);
    }}

}}

global proc check_test_cases(string $test_case, int $pass, string $script_info, string $scene){{

    string $tests = "{testCases}";
    string $test[];
    $numTokens = `tokenize $tests "," $test`;

    if ($tests != "all") {{
        for ($each in $test) {{ 
            if ($each == $test_case) {{
                prerender($test_case, $pass, $script_info, $scene);
            }}
        }}
    }} else {{
        prerender($test_case, $pass, $script_info, $scene);
    }}
}}


global proc prerender(string $test_case, int $pass, string $script_info, string $scene){{

	string $scene_name  = `file -q -sn -shn`;
	if ($scene_name != $scene) {{
		if (catch (`file -f -options "v=0;"  -ignoreVersion -o $scene`)) {{
            evalDeferred("quit -abort");
        }} else {{
        	activate_jpg_format();
        }}
	}}

	if( `pluginInfo -query -loaded RadeonProRender` == 0){{
		loadPlugin RadeonProRender;
	}}

	if( `pluginInfo -query -loaded fbxmaya` == 0){{
    	loadPlugin fbxmaya;
	}}

	setAttr "defaultRenderGlobals.currentRenderer" -type "string" "FireRender";
	setAttr "RadeonProRenderGlobals.completionCriteriaIterations" $pass;

	switch ($test_case) {{

	case "MAYA_SM_003":
		import_fbx();
		rpr_render($test_case, $script_info);
		delete_fbx();
		break;

	case "MAYA_SM_004":
		import_obj();
		rpr_render($test_case, $script_info);
		delete_obj();
		break;

	case "MAYA_SM_006":
		create_IBL();
		rpr_render($test_case, $script_info);
		delete_IBL();
		break;

	case "MAYA_SM_007":
		create_IBL();
		update_IBL_hdr();
		rpr_render($test_case, $script_info);
		delete_IBL();
		break;

	case "MAYA_SM_008":
		create_IBL();
		update_IBL_exr();
		rpr_render($test_case, $script_info);
		delete_IBL();
		break;

	case "MAYA_SM_009":
		create_sun_sky();
		rpr_render($test_case, $script_info);
		delete_sun_sky();
		break;

	case "MAYA_SM_010":
		create_ies_light();
		rpr_render($test_case, $script_info);
		delete_ies_light();
		break;

	case "MAYA_SM_014":
		activate_tone_mapping();
		rpr_render($test_case, $script_info);
		deactivate_tone_mapping();
		break;

	case "MAYA_SM_015":
		activate_render_stamp();
		rpr_render($test_case, $script_info);
		deactivate_render_stamp();
		break;

	case "MAYA_SM_016":
		activate_wireframe_mode();
		rpr_render($test_case, $script_info);
		deactivate_wireframe_mode();
		break;

	case "MAYA_SM_017":
		activate_medium_quality();
		rpr_render($test_case, $script_info);
		deactivate_medium_quality();
		break;

	case "MAYA_SM_018":
		change_image_size_hd720();
		rpr_render($test_case, $script_info);
		change_image_size_standart();
		break;

	case "MAYA_SM_019":
		change_image_size_custom();
		rpr_render($test_case, $script_info);
		change_image_size_standart();
		break;

	case "MAYA_SM_020":
		activate_png_format();
		rpr_render($test_case, $script_info);
		sysFile -copy ("{work_dir}/Color/MAYA_SM_020.jpg") ("{work_dir}" + "/../../../../jobs/Tests/pass.jpg");
		break;

	case "MAYA_SM_021":
		activate_jpg_format();
		rpr_render($test_case, $script_info);
		break;

	case "MAYA_SM_022":
		activate_denoiser_bilateral();
		rpr_render($test_case, $script_info);
		deactivate_denoiser();
		break;

	case "MAYA_SM_023":
		activate_denoiser_lwr();
		rpr_render($test_case, $script_info);
		deactivate_denoiser();
		break;

	case "MAYA_SM_024":
		activate_denoiser_eaw();
		rpr_render($test_case, $script_info);
		deactivate_denoiser();
		break;

	case "MAYA_SM_025":
		create_area_light();
		rpr_render($test_case, $script_info);
		delete_area_light();
		break;

	case "MAYA_SM_026":
		activate_uv();
		rpr_render($test_case, $script_info);
		deactivate_uv();
		break;

	case "MAYA_SM_027":
		create_physical_light();
		rpr_render($test_case, $script_info);
		delete_physical_light();
		break;

	case "MAYA_SM_028":
		import_rpr_matlib();
		rpr_render($test_case, $script_info);
		break;

	case "MAYA_SM_029":
		create_and_asign_pbr();
		rpr_render($test_case, $script_info);
		break;

	case "MAYA_SM_030":
		create_and_asign_uber();
		rpr_render($test_case, $script_info);
		break;

	case "MAYA_SM_031":
		duplicate_500_instances();
		rpr_render($test_case, $script_info);
		break;

	case "MAYA_SM_032":
		multiple_ies_lights();
		rpr_render($test_case, $script_info);
		break;

	case "MAYA_SM_033":
		shadow_catcher_test();
		rpr_render($test_case, $script_info);
		break;

	case "MAYA_SM_035":
		maxRayDepth_15();
		setIterations_400();
		rpr_render($test_case, $script_info);
		maxRayDepth_default();
		setIterations_default();
		break;

	default:
		rpr_render($test_case, $script_info);
		break;

	}}
	
			
}}


global proc check_rpr_load(){{
	if( `pluginInfo -query -loaded RadeonProRender` == 0){{
    	loadPlugin RadeonProRender;
	}}

	if( `pluginInfo -query -loaded fbxmaya` == 0){{
    	loadPlugin fbxmaya;
	}}

	setAttr "defaultRenderGlobals.currentRenderer" -type "string" "FireRender";
}}

