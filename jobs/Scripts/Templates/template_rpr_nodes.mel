	    rpr_render($test_case, $script_info);
        lookThroughModelPanel Camera_Checker_Dot_Texture modelPanel4;
        setAttr ("BlendUbernPBRMaterials.visibility") 0;
        setAttr ("CheckerDotTexture.visibility") 1;
        break;

