    @check_test_cases("MAYA_RS_QL_018", "[\"Max Ray Depth - 50, Max Diffse - 50, Max Glossy - 50, Max Refraction - 50, Max Glossy Refraction - 50, Max Shadow - 50, Ray Epsilon - 2\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_019", "[\"Clamp Irradience Value - 50\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_020", "[\"Clamp Irradience Value - 100\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_021", "[\"Clamp Irradience Value - 50, Activate Texture Compression checkbox\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_022", "[\" Clamp Irradience Value - 100, Activate Texture Compression checkbox\"]", "maya_shaderball.ma");
    
    // <-- end -->

	evalDeferred("quit -abort");
}}
