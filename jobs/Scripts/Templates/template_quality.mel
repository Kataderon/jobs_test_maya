    @check_test_cases("MAYA_RS_QL_013", "[\" Ray Epsilon - 0\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_014", "[\" Ray Epsilon - 2\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_015", "[\"Activate Enable Out of Core Textures checkbox\"]", "maya_shaderball.ma");
