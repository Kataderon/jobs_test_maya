    @check_test_cases("MAYA_RS_QL_007", "[\" Max Refraction - 2\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_008", "[\" Max Refraction - 50\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_009", "[\" Max Glossy Refraction - 2\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_010", "[\" Max Glossy Refraction - 50\"]", "maya_shaderball.ma");
    @check_test_cases("MAYA_RS_QL_011", "[\" Max Shadow - 2\"]", "maya_shaderball.ma");
